

def pythagorean_triangle(a, b, c):
    if a**2 + b**2 == c**2:
        return "This is a pythagorean triangle"
    else:
        return "This is not a pythagorean triangle"

a = raw_input("Enter the first side: ")
b = raw_input("Enter the second side: ")
c = raw_input("Enter the hypotenuse: ")

print pythagorean_triangle(int(a), int(b), int(c))

